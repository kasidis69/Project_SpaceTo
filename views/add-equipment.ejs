<%- include("./partials/header.ejs") %>

            <% if (message){%>
                <p class="text-center text-danger">
                    <%-message%>
                    </p>
            <% } %>
                    
            
	<div class="container-contact100">
		<div class="wrap-contact100">
			<form  class="contact100-form validate-form" action="" method="post" enctype="multipart/form-data">
				<span class="contact100-form-title">
					Add Equipment
				</span>

				<div class="wrap-input100 validate-input bg1" data-validate="Please Type Your Equipment Name">
					<span class="label-input100">Equipment Name *</span>
					<input name="equipment_name" class="input100" type="text"  placeholder="Enter Your Equipment Name" required>
				</div>

                <div class="wrap-input100 validate-input bg1" data-validate="Please Type Your Name">
					<span class="label-input100">Equipment Brand *</span>
					<input name="equipment_brand" class="input100" type="text" name="equipment_name" placeholder="Enter Equipment Brand" required>
				</div>

                <div class="wrap-input100 validate-input bg1" data-validate="Please Type Your Name">
					<span class="label-input100">Equipment Model *</span>
					<input class="input100" type="text" name="equipment_model"  placeholder="Enter Your Equipment Model" required>
				</div>

    
                <div class="wrap-input100 validate-input bg1" data-validate="Please Type Your Name">
					<span class="label-input100">Equipment Serial ID *</span>
					<input class="input100" type="text" name="equipment_serial_id"  placeholder="Enter Your Equipment Serial ID ">
				</div>


				<div class="container-contact100-form-btn">
					<button type="submit"  class="contact100-form-btn">
						<span>
							Submit
							<i class="fa fa-long-arrow-right m-l-7" aria-hidden="true"></i>
						</span>
					</button>
					
				</div>
			</form>
		</div>
	</div>


    